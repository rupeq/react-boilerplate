{
	"name": "boilerplate",
	"private": true,
	"version": "1.0.0",
	"description": "Starter template for React",
	"type": "module",
	"author": "artem.dereviago <multimouse123@gmail.com>",
	"license": "MIT",
	"scripts": {
		"generate-routes": "tsr generate",
		"watch-routes": "tsr watch",
		"setup": "pnpm install && git init && npx husky && npx playwright install && pnpm run generate-routes",
		"prepare": "husky",
		"husky": "node ./node_modules/husky/bin.js",
		"lint:code": "eslint --max-warnings 0 --cache \"**/*.{js,jsx,ts,tsx}\"",
		"lint:styles": "stylelint \"**/*.{scss,css}\"",
		"lint:types": "tsc --noEmit",
		"lint:format": "prettier --cache \"**/*.{jsx,ts,tsx,css,scss,json,md,yaml,yml}\"",
		"lint:fix": "pnpm run lint:code --fix && pnpm run lint:format --write && pnpm run lint:styles --fix",
		"precommit": "lint-staged",
		"commitlint": "commitlint --edit",
		"test:unit": "vitest src/",
		"test:unit:coverage": "vitest --coverage src/",
		"test:e2e": "playwright test",
		"test:e2e:report": "playwright show-report",
		"test": "vitest run src/ && playwright test",
		"dev": "vite",
		"build": "pnpm run generate-routes && tsc && vite build",
		"preview": "vite preview",
		"commit": "commit"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.6.1",
		"@commitlint/config-conventional": "^19.6.0",
		"@commitlint/prompt-cli": "^19.7.0",
		"@commitlint/types": "^19.5.0",
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.19.0",
		"@faker-js/faker": "^9.4.0",
		"@ianvs/prettier-plugin-sort-imports": "^4.4.1",
		"@playwright/test": "^1.50.0",
		"@tailwindcss/vite": "^4.0.0",
		"@tanstack/router-cli": "^1.97.17",
		"@tanstack/router-devtools": "^1.97.17",
		"@tanstack/router-plugin": "^1.97.17",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^16.2.0",
		"@testing-library/user-event": "^14.6.1",
		"@total-typescript/ts-reset": "^0.6.1",
		"@types/eslint__js": "^8.42.3",
		"@types/jest": "^29.5.14",
		"@types/node": "^22.10.10",
		"@types/react": "^19.0.8",
		"@types/react-dom": "^19.0.3",
		"@typescript-eslint/eslint-plugin": "^8.21.0",
		"@typescript-eslint/parser": "^8.21.0",
		"@vitejs/plugin-react-swc": "^3.7.2",
		"@vitest/coverage-v8": "^3.0.4",
		"autoprefixer": "^10.4.20",
		"eslint": "^9.19.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-import-resolver-typescript": "^3.7.0",
		"eslint-plugin-import-x": "^4.6.1",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"eslint-plugin-react": "^7.37.4",
		"eslint-plugin-react-hooks": "^5.1.0",
		"eslint-plugin-react-refresh": "^0.4.18",
		"eslint-plugin-unicorn": "^56.0.1",
		"globals": "^15.14.0",
		"husky": "^9.1.7",
		"jsdom": "^26.0.0",
		"lint-staged": "^15.4.2",
		"prettier": "^3.4.2",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"stylelint": "^16.13.2",
		"stylelint-order": "^6.0.4",
		"stylelint-prettier": "^5.0.2",
		"stylelint-scss": "^6.10.1",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.21.0",
		"vite": "^6.0.11",
		"vite-plugin-static-copy": "^2.2.0",
		"vitest": "^3.0.4",
		"vitest-preview": "^0.0.1"
	},
	"dependencies": {
		"@tanstack/react-router": "^1.97.17",
		"i18next": "^24.2.2",
		"i18next-browser-languagedetector": "^8.0.2",
		"i18next-http-backend": "^3.0.2",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-i18next": "^15.4.0"
	},
	"engines": {
		"node": ">=22.0.0",
		"pnpm": ">=9.0.0"
	},
	"packageManager": "pnpm@9.15.4+sha512.b2dc20e2fc72b3e18848459b37359a32064663e5627a51e4c74b2c29dd8e8e0491483c3abb40789cfd578bf362fb6ba8261b05f0387d76792ed6e23ea3b1b6a0"
}
